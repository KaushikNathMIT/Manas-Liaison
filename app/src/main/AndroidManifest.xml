<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="projectmanas.in.manasliaison">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activities.HomeActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.ProfileActivity" />
        <activity android:name=".Activities.OnlineChallengeCommingSoon" />
        <activity android:name=".Activities.OnlineChallengeOngoing" />
        <activity android:name=".Activities.OrientationActivity" />
        <activity
            android:name=".Activities.InterviewActivity"
            android:label="@string/title_activity_interview"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>